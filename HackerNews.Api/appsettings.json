{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HackerNewsApiSettings": {
    "BaseUrl": "https://hacker-news.firebaseio.com/",
    "RetryCount": 3
  },
  "CacheSettings": {
    "QueryResultTtlInSeconds": 60,
    "BestStoriesIdsTtlInSeconds": 60,
    "StoryDetailsTtlInSeconds": 300
  }
}
